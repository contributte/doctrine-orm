{
  "name": "nettrine/orm",
  "description": "Doctrine ORM for Nette Framework",
  "keywords": ["nette", "doctrine", "orm", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/nettrine/orm",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "contributte/di": "^0.5.0",
    "symfony/console": "^4.2.5|^5.0.0",
    "nettrine/annotations": "^0.7.0 || ^0.8.0",
    "nettrine/cache": "^0.3.0 || ^0.4.0",
    "nettrine/dbal": "^0.7.0 || ^0.8.0",
    "doctrine/orm": "^2.6.3",
    "doctrine/common":"^2.13.1 || ^3.0.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.4",
    "phpstan/phpstan": "^0.12.64",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-nette": "^0.12.3",
    "phpstan/phpstan-strict-rules": "^0.12.2"
  },
  "conflict": {
    "nette/di": "<3.0.6",
    "nette/schema": "<1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\ORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures",
      "Tests\\Toolkit\\": "tests/toolkit"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8.x-dev"
    }
  }
}
