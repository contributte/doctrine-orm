{
  "name": "nettrine/orm",
  "description": "Doctrine ORM for Nette Framework",
  "keywords": ["nette", "doctrine", "orm", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-orm",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "doctrine/orm": "^3.3.0",
    "nette/di": "^3.1.2",
    "nettrine/dbal": "^0.10.2 || ^0.11.0",
    "psr/cache": "^3.0.0",
    "psr/log": "^3.0.2"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2.0",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3.0",
    "symfony/console": "^7.1.8 ",
    "symfony/cache": "^7.1.9",
    "monolog/monolog": "^3.8.0",
    "mockery/mockery": "^1.6.12",
    "tracy/tracy": "^2.10.3"
  },
  "conflict": {
    "doctrine/event-manager": "<2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\ORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.10.x-dev"
    }
  }
}
